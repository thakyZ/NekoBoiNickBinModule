  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell: Test Profile",
      "type": "PowerShell",
      "request": "launch",
      "script": "${env:UserProfile}\\Documents\\PowerShell\\Microsoft.PowerShell_profile.ps1",
      "args": []
    },
    {
      "name": "PowerShell: Test Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": []
    },
    {
      "name": "PowerShell: Test Export-ChocoConfig",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}\\Export-ChocoConfig.ps1",
      "args": [
        "-Path $HOME\\Downloads\\n\\choco-export.xml",
        "-SaveArguments",
        "-IncludeVersion"
      ]
    },
    {
      "name": "PowerShell: Test Export-ChocoConfig",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}\\Export-ChocoConfig.ps1",
      "args": [
        "-Path $HOME\\Downloads\\n\\choco-export.xml",
        "-SaveArguments",
        "-IncludeVersion"
      ]
    },
    {
      "name": "PowerShell: Test Update-ChocoPackage.test",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}\\.vscode\\test\\Update-ChocoPackage.test.ps1",
      "args": []
    },
    {
      "name": "PowerShell: Test Unblock-Files",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}\\Unblock-Files.ps1",
      "args": [
        "-Path $HOME\\Pictures -Verbose -Recurse"
      ]
    },
    {
      "name": "PowerShell: Test Sort-ItemsInU",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}\\Sort-ItemsInU.ps1",
      "args": [
        "-Debug"
      ],
    }
  ]
}